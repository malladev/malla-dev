{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": "off",
        "jsx-quotes": "warn",
        "no-console": "warn",
        "no-mixed-spaces-and-tabs": 0,
        "no-unused-vars": "off",
        "no-warning-comments": 0,
        "react/react-in-jsx-scope": "off",
        "no-useless-escape": "off",
        "react/prop-types": "off",
        "jsx-a11y/img-redundant-alt":"off"
    }
}